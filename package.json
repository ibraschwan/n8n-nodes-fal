{
  "name": "@ibracob.dev/n8n-nodes-falai",
  "version": "0.1.1",
  "description": "n8n node for Fal.ai - LLM text generation, image/video generation, image editing, and vision AI models",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "fal",
    "fal.ai",
    "ai",
    "llm",
    "gpt",
    "claude",
    "gemini",
    "openai",
    "anthropic",
    "google",
    "text-generation",
    "image-generation",
    "video-generation",
    "image-editing",
    "video-editing",
    "vision",
    "workflow",
    "queue",
    "sora",
    "veo",
    "flux",
    "stable-diffusion",
    "upscaling",
    "background-removal",
    "automation",
    "ml",
    "machine-learning",
    "deep-learning",
    "diffusion"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ibraschwan/n8n-nodes-falai",
  "author": {
    "name": "ibracob.dev",
    "email": "ben@ibracob.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibraschwan/n8n-nodes-falai.git"
  },
  "bugs": {
    "url": "https://github.com/ibraschwan/n8n-nodes-falai/issues"
  },
  "main": "index.js",
  "scripts": {
    "build": "n8n-node build && cp nodes/Fal/Fal.node.json dist/nodes/Fal/",
    "dev": "n8n-node dev",
    "format": "prettier nodes credentials --write",
    "lint": "n8n-node lint",
    "lintfix": "n8n-node lint --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": [
      "dist/credentials/FalApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Fal/Fal.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "@types/node": "^20.10.0",
    "eslint": "^9.32.0",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
